<%- model_class = Quote -%>
  <h1>Popular</h1>
<table class="table table-striped">
  <tbody>
    <% @quotes.each do |quote| %>
      <tr>
        <td><%= link_to quote.text, quote_path(quote) %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_quote_path,
            :class => 'btn btn-primary' %>
